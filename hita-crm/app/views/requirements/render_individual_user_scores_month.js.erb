var months = <%= raw @months %>;
var scores = <%= raw @scores %>;

$.each(months, function(k, v) {
  $(`#monthSlot${k + 1}`).html(v);
});

$.each(scores, function(k, v) {
  $(`#monthScoreSlot${k}`).html(`${v} pts`);
});

$("#monthScoreTotalSlot").html("<%= @user_scores_semester %> pts");

$('.requirements').html('<%= escape_javascript(render(:partial => "requirements/requirement", :locals => { :requirements => @requirements, :year => @year, :period => @period, :user => @user, :criterion => @criterion, :manage_criterion => true })) %>');
