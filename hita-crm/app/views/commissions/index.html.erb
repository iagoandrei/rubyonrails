<p id="notice"><%= notice %></p>

<h1>Commissions</h1>

<table>
  <thead>
    <tr>
      <th>Represent</th>
      <th>Value</th>
      <th>Paydate</th>
      <th>Paid</th>
      <th>Request</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commissions.each do |commission| %>
      <tr>
        <td><%= commission.represent %></td>
        <td><%= commission.value %></td>
        <td><%= commission.paydate %></td>
        <td><%= commission.paid %></td>
        <td><%= commission.request_id %></td>
        <td><%= link_to 'Show', commission %></td>
        <td><%= link_to 'Edit', edit_commission_path(commission) %></td>
        <td><%= link_to 'Destroy', commission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commission', new_commission_path %>
